#include <stdio.h>
#include <cs50.h>
#include <ctype.h>
#include <string.h>

int main(void){
    
    
    string name = get_string();
    
    
    for(int i = 0; i < strlen(name) ; i++){
        if (name[i] == ' ' && name[i+1] != ' '){
            printf("%c",toupper(name[i+1]));
        }
        
            
        
    }
    
    printf("\n");
}
